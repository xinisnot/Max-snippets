								"box" : 								{
									"code" : "//============================================================\n// Phase distortion for window function\n\n//============================================================\n// functions\n\nscaleU2X(x, from, to)\n{\n    return x*(to-from) + from;\n}\n\nscaleX2U(x, from, to)\n{\n    return (x-from) / (to-from);\n}\n\nscaleX2Y(x, xFrom, xTo, yFrom, yTo)\n{\n    return scaleU2X(scaleX2U(x, xFrom, xTo), yFrom, yTo);\n}\n\nshiftHalf(x, a)\n{\n    return x<a ? scaleX2Y(x, 0, a, 0, 0.5)\n               : scaleX2Y(x, a, 1, 0.5, 1);\n}\n\ncompressHalfVert(x, a)\n{\n    v = a * 0.5;\n    y = 0;\n\n    if(x>v && x<1-v)\n    {\n        y = scaleX2Y(x, v, 1-v, 0, 1);\n    }\n    else if(x<v)\n    {\n        y = 0;\n    }\n    else\n    {\n        y = 1;\n    }\n\n    return y;\n}\n\ncompressHalfHor(x, a)\n{\n    v = (1-a)*0.5;\n    y = 0;\n\n    if(x<v)\n    {\n        y = scaleX2Y(x, 0, v, 0, 0.5);\n    }\n    else if(x>1-v)\n    {\n        y = scaleX2Y(x, 1-v, 1, 0.5, 1);\n    }\n    else\n    {\n        y = 0.5;\n    }\n\n    return y;\n}\n\nphaseDistortionW(x, half, vert, hor)\n{\n    v = shiftHalf(x, half);\n    v = compressHalfVert(v, vert);\n    v = compressHalfHor(v, hor);\n\n    return v;\n}\n\n//============================================================\n// parameters\n\nParam half(0.5, min=0, max=1);\nParam quarter(0.5, min=0, max=1);\nParam vert(0, min=0, max=1);\nParam hor(0, min=0, max=1);\nParam rep1(1, min=0);\nParam rep2(1, min=0);\n\n//============================================================\n// main loop\n\nout1 = phaseDistortionW(in1, half, vert, hor);\n",
