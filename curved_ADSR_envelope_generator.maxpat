						"boxes" : [ 							{
								"box" : 								{
									"code" : "//============================================================\n// function\n\ncurve(x, from, to, amt)\n{\n    // scale x from [from, to] to [0, 1]\n    y = (x-from) / (to-from);\n\n    if(amt>0)\n    {\n        y = exp(ln(y) * (1/(1-amt)));\n    }\n    else\n    {\n        y = exp(ln(y) * (1+amt));\n    }\n\n    // scale y from [0, 1] to [to, from]\n    return y * (to-from) + from;\n}\n\nADSREnvelopeGenerator(gate, time_attack, time_decay, time_release, curve_attack, curve_decay, curve_release, sustain, sensitivity)\n{\n    History yLinear(0);\n    History yCurve(0);\n    History dAttack(0);\n    History dDecay(0);\n    History dRelease(0);\n    History lPeak(0);\n    History lSustain(0);\n    History amtCurveAttack(0);\n    History amtCurveDecay(0);\n    History amtCurveRelease(0);\n    History breakpoint(0);\n\n    // The state of the envelope generator is managed by breakpoint.\n    //  breakpoint==0 -> free\n    //  breakpoint==1 -> during the attack time\n    //  breakpoint==2 -> during the decay time\n    //  breakpoint==3 -> state of sustain\n    //  breakpoint==4 -> during the release time\n\n    if(gate!=0)\n    {\n        // when envelope generator is free\n        if(breakpoint==0)\n        {\n            lPeak           = 1 - (1-gate) * sensitivity;\n            lSustain        = lPeak * sustain;\n            yLinear         = 0;\n            yCurve          = 0;\n            dAttack         = lPeak            / mstosamps(time_attack);\n            dDecay          = (lSustain-lPeak) / mstosamps(time_decay);\n            amtCurveAttack  = curve_attack;\n            amtCurveDecay   = curve_decay;\n            breakpoint      = 1;\n        }\n        // when envelope generator is during the release time\n        else if(breakpoint==4)\n        {\n            lPeak           = 1 - (1-gate) * sensitivity;\n            lSustain        = lPeak * sustain;\n            dAttack         = (lPeak-yLinear)  / mstosamps(time_attack);\n            dDecay          = (lSustain-lPeak) / mstosamps(time_decay);\n            amtCurveAttack  = curve_attack;\n            amtCurveDecay   = curve_decay;\n            breakpoint      = 1;\n        }\n    }\n    else\n    {\n        // when envelope generator is state of sustain\n        if(breakpoint==3)\n        {\n            dRelease        = -lSustain / mstosamps(time_release);\n            amtCurveRelease = curve_release;\n            breakpoint      = 4;\n        }\n        // when envelope generator is during the attack or decay\n        else if(breakpoint==1 || breakpoint==2)\n        {\n            dRelease        = -yLinear / mstosamps(time_release);\n            amtCurveRelease = curve_release;\n            breakpoint      = 4;\n        }\n    }\n\n    y0 = yCurve;\n\n    // update of envelope generator\n    // when attack\n    if(breakpoint==1)\n    {\n        if(dAttack!=0)\n        {\n            yLinear = clip(yLinear+dAttack, 0, lPeak);\n            yCurve  = amtCurveAttack!=0 ? curve(yLinear, 0, lPeak, amtCurveAttack) : yLinear;\n        }\n        else\n        {\n            yLinear = lPeak;\n            yCurve  = lPeak;\n        }\n\n        if(yLinear>=lPeak)\n        {\n            breakpoint = 2;\n        }\n    }\n    // when decay\n    else if(breakpoint==2)\n    {\n        if(dDecay!=0)\n        {\n            yLinear = clip(yLinear+dDecay, lSustain, lPeak);\n            yCurve  = amtCurveDecay!=0 ? curve(yLinear, lPeak, lSustain, amtCurveDecay) : yLinear;\n        }\n        else\n        {\n            yLinear = lSustain;\n            yCurve  = lSustain;\n        }\n\n        if(yLinear<=lSustain)\n        {\n            breakpoint = 3;\n        }\n    }\n    // when sustain\n    else if(breakpoint==3)\n    {\n        yLinear = lSustain;\n        yCurve  = lSustain;\n    }\n    // when release\n    else if(breakpoint==4)\n    {\n        if(dRelease!=0)\n        {\n            yLinear = clip(yLinear+dRelease, 0, lSustain);\n            yCurve  = amtCurveRelease!=0 ? curve(yLinear, lSustain, 0, amtCurveRelease) : yLinear;\n        }\n        else\n        {\n            yLinear = 0;\n            yCurve  = 0;\n        }\n\n        if(yLinear<=0)\n        {\n            breakpoint = 0;\n        }\n    }\n\n    return y0;\n}\n\n//============================================================\n// parameters\n\nParam time_attack(5, min=0);\nParam time_decay(80, min=0);\nParam time_release(300, min=0);\nParam curve_attack(0, min=-1, max=1);\nParam curve_decay(0, min=-1, max=1);\nParam curve_release(0, min=-1, max=1);\nParam sustain(0.25, min=0, max=1);\nParam sensitivity(1, min=0, max=1);\n\nout1 = ADSREnvelopeGenerator(in1, time_attack, time_decay, time_release, curve_attack, curve_decay, curve_release, sustain, sensitivity);",
