, 							{
								"box" : 								{
									"code" : "//============================================================\n// function\n\n// curve(x, from, to, amt)\n// {\n//     // scale x from [from, to] to [0, 1]\n//     y = (x-from) / (to-from);\n\n//     if(amt>0)\n//     {\n//         y = exp(ln(y) * (1/(1-amt)));\n//     }\n//     else\n//     {\n//         y = exp(ln(y) * (1+amt));\n//     }\n\n//     // scale y from [0, 1] to [to, from]\n//     return y * (to-from) + from;\n// }\n\nAdsrEnvelopeGenerator(gate, l0, lP, lS, lT, s0, sP, sS, sT, tA, tD, tR, cA, cD, cR)\n{\n    History y0(0);\n    History deltaA(0);\n    History deltaD(0);\n    History deltaR(0);\n    History curveA(0);\n    History curveD(0);\n    History curveR(0);\n    History level0(0);\n    History levelP(0);\n    History levelS(0);\n    History levelT(0);\n    History breakpoint(0);\n\n    // The state of the envelope generator is managed by breakpoint.\n    //  breakpoint==0 -> free\n    //  breakpoint==1 -> during the attack time\n    //  breakpoint==2 -> during the decay time\n    //  breakpoint==3 -> state of sustain\n    //  breakpoint==4 -> during the release time\n\n    // gate is on\n    if(gate!=0)\n    {\n        // when envelope generator is free\n        if(breakpoint==0)\n        {\n            level0 = l0 * (1 - (1-gate) * s0);\n            levelP = lP * (1 - (1-gate) * sP);\n            levelS = lS * (1 - (1-gate) * sS);\n            curveA = cA;\n            curveD = cD;\n            deltaA = (levelP-level0) / mstosamps(tA);\n            deltaD = (levelS-levelP) / mstosamps(tD);\n            breakpoint = 1;\n        }\n        // when envelope generator is during the release time\n        else if(breakpoint==4)\n        {\n            level0 = y0;\n            levelP = lP * (1 - (1-gate) * sP);\n            levelS = lS * (1 - (1-gate) * sS);\n            curveA = cA;\n            curveD = cD;\n            deltaA = (levelP-level0) / mstosamps(tA);\n            deltaD = (levelS-levelP) / mstosamps(tD);\n            breakpoint = 1;\n        }\n    }\n    // gate is off\n    else\n    {\n        // when envelope generator is state of sustain\n        if(breakpoint==3)\n        {\n            levelT = lT;\n            curveR = cR;\n            deltaR = (levelT-levelS) / mstosamps(tR);\n            breakpoint = 4;\n        }\n        // when envelope generator is during the attack or decay\n        else if(breakpoint==1 || breakpoint==2)\n        {\n            levelT = lT;\n            curveR = cR;\n            deltaR = (levelT-y0) / mstosamps(tR);\n            breakpoint = 4;\n        }\n    }\n\n    y = y0;\n\n    // update of envelope generator\n    // when attack\n    if(breakpoint==1)\n    {\n        if(deltaA!=0)\n        {\n            y0 = clip(y0+deltaA, 0, levelP);\n        }\n        else\n        {\n            y0 = levelP;\n        }\n\n        if(y>=levelP)\n        {\n            breakpoint = 2;\n        }\n    }\n    // when decay\n    else if(breakpoint==2)\n    {\n        if(deltaD!=0)\n        {\n            y0 =  clip(y0+deltaD, levelS, levelP);\n        }\n        else\n        {\n            y0 = levelS;\n        }\n\n        if(y0<=levelS)\n        {\n            breakpoint = 3;\n        }\n    }\n    else if(breakpoint==3)\n    {\n        y0 = levelS;\n    }\n    else if(breakpoint==4)\n    {\n        if(deltaR!=0)\n        {\n            y0 = clip(y0+deltaR, levelT, levelS);\n        }\n        else\n        {\n            y0 = levelT;\n        }\n\n        if(y0<=levelT)\n        {\n            breakpoint = 0;\n        }\n    }\n\n    return y;\n}\n\n// ADSREnvelopeGenerator(gate, time_attack, time_decay, time_release, curve_attack, curve_decay, curve_release, sustain, sensitivity)\n// {\n//     History yLinear(0);\n//     History yCurve(0);\n//     History dAttack(0);\n//     History dDecay(0);\n//     History dRelease(0);\n//     History lPeak(0);\n//     History lSustain(0);\n//     History amtCurveAttack(0);\n//     History amtCurveDecay(0);\n//     History amtCurveRelease(0);\n//     History breakpoint(0);\n\n//     // The state of the envelope generator is managed by breakpoint.\n//     //  breakpoint==0 -> free\n//     //  breakpoint==1 -> during the attack time\n//     //  breakpoint==2 -> during the decay time\n//     //  breakpoint==3 -> state of sustain\n//     //  breakpoint==4 -> during the release time\n\n//     if(gate!=0)\n//     {\n//         // when envelope generator is free\n//         if(breakpoint==0)\n//         {\n//             lPeak           = 1 - (1-gate) * sensitivity;\n//             lSustain        = lPeak * sustain;\n//             yLinear         = 0;\n//             yCurve          = 0;\n//             dAttack         = lPeak            / mstosamps(time_attack);\n//             dDecay          = (lSustain-lPeak) / mstosamps(time_decay);\n//             amtCurveAttack  = curve_attack;\n//             amtCurveDecay   = curve_decay;\n//             breakpoint      = 1;\n//         }\n//         // when envelope generator is during the release time\n//         else if(breakpoint==4)\n//         {\n//             lPeak           = 1 - (1-gate) * sensitivity;\n//             lSustain        = lPeak * sustain;\n//             dAttack         = (lPeak-yLinear)  / mstosamps(time_attack);\n//             dDecay          = (lSustain-lPeak) / mstosamps(time_decay);\n//             amtCurveAttack  = curve_attack;\n//             amtCurveDecay   = curve_decay;\n//             breakpoint      = 1;\n//         }\n//     }\n//     else\n//     {\n//         // when envelope generator is state of sustain\n//         if(breakpoint==3)\n//         {\n//             dRelease        = -lSustain / mstosamps(time_release);\n//             amtCurveRelease = curve_release;\n//             breakpoint      = 4;\n//         }\n//         // when envelope generator is during the attack or decay\n//         else if(breakpoint==1 || breakpoint==2)\n//         {\n//             dRelease        = -yLinear / mstosam