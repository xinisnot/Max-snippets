						"boxes" : [ 							{
								"box" : 								{
									"code" : "//============================================================\n// Oscillator using phase distortion.\n\n//============================================================\n// functions\n\nscaleU2X(x, from, to)\n{\n    return x*(to-from) + from;\n}\n\nscaleX2U(x, from, to)\n{\n    return (x-from) / (to-from);\n}\n\nscaleX2Y(x, xFrom, xTo, yFrom, yTo)\n{\n    return scaleU2X(scaleX2U(x, xFrom, xTo), yFrom, yTo);\n}\n\nshiftHalf(x, a)\n{\n    return x<a ? scaleX2Y(x, 0, a, 0, 0.5)\n               : scaleX2Y(x, a, 1, 0.5, 1);\n}\n\nshiftQuarter(x, a)\n{\n    p = wrap(x*2, 0, 1);\n\n    return x<0.5 ? shiftHalf(p, a) * 0.5\n                 : shiftHalf(p, 1-a) * 0.5 + 0.5;\n}\n\ncompressHalfVert(x, a)\n{\n    v = a * 0.5;\n    y = 0;\n\n    if(x>v && x<1-v)\n    {\n        y = scaleX2Y(x, v, 1-v, 0, 1);\n    }\n    else if(x<v)\n    {\n        y = 0;\n    }\n    else\n    {\n        y = 1;\n    }\n\n    return y;\n}\n\ncompressQuarterVert(x, a)\n{\n    return x<0.5 ? compressHalfVert(x*2,       a) * 0.5\n                 : compressHalfVert((x-0.5)*2, a) * 0.5 + 0.5;\n}\n\ncompressHalfHor(x, a)\n{\n    v = (1-a)*0.5;\n    y = 0;\n\n    if(x<v)\n    {\n        y = scaleX2Y(x, 0, v, 0, 0.5);\n    }\n    else if(x>1-v)\n    {\n        y = scaleX2Y(x, 1-v, 1, 0.5, 1);\n    }\n    else\n    {\n        y = 0.5;\n    }\n\n    return y;\n}\n\ncompressQuarterHor(x, a)\n{\n    p = wrap(x*2, 0, 1);\n\n    return x<0.5 ? compressHalfHor(p, a) * 0.5\n                 : compressHalfHor(p, a) * 0.5 + 0.5;\n}\n\nrepeat(x, n1, n2)\n{\n    History prev(0);\n    History select(1);\n\n    p = wrap(x*2, 0, 1);\n\n    if(p - prev < 0)\n    {\n        select = wrap(select+1, 1, 3);\n    }\n\n    y1, y2 = gate(select, p, choices=2);\n\n    y1 = scaleU2X(wrap(y1*n1, 0, 1), 0, 0.5);\n    y2 = scaleU2X(wrap(y2*n2, 0, 1), 0.5, 1);\n\n    prev = p;\n\n    return selector(select, y1, y2, choices=2);\n}\n\nphaseDistortion(x, half, quarter, vert, hor, rep1, rep2)\n{\n    v = repeat(x, rep1, rep2);\n    v = shiftHalf(v, half);\n    v = shiftQuarter(v, quarter);\n    v = compressQuarterHor(v, hor);\n    v = compressQuarterVert(v, vert);\n\n\n    return v;\n}\n\npdOsc(buf, x, half, quarter, vert, hor, rep1, rep2)\n{\n    p = phaseDistortion(x, half, quarter, vert, hor, rep1, rep2);\n\n    return nearest(buf, p);\n}\n\n//============================================================\n// buffers\n\nBuffer buf_osc(\"\");\n\n//============================================================\n// parameters\n\nParam half(0.5, min=0, max=1);\nParam quarter(0.5, min=0, max=1);\nParam vert(0, min=0, max=1);\nParam hor(0, min=0, max=1);\nParam rep1(1, min=0);\nParam rep2(1, min=0);\n\n//============================================================\n// main loop\n\nout1 = pdOsc(buf_osc, in1, half, quarter, vert, hor, rep1, rep2);",
