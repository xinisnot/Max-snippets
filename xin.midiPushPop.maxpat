								"box" : 								{
									"code" : "//============================================================\n// Push or pop MIDI notes to 2-channel buffer.\n// The channel assignments are\n//   1ch -> pitch\n//   2ch -> velocity\n\n//============================================================\n// functions\n\nindexOf(buf, x)\n{\n    itr = 0;\n    \n    while(itr!=dim(buf))\n    {\n        if(peek(buf, itr)==x)\n        {\n            break;\n        }\n\n        itr += 1;\n    }\n\n    return itr;\n}\n\n\nmidiPushPop(buf, pitch, velocity)\n{\n    History next_index(0);\n\n    // when note on, push pitch and velocity to buf_midi\n    if(velocity!=0)\n    {\n        poke(buf, pitch, next_index, 0);\n        poke(buf, pitch, next_index, 1);\n        next_index += 1;\n    }\n    else\n    // when note off, pop from buf_midi and overwrite pitch and velocity\n    {\n        idx = indexOf(buf, pitch);\n\n        if(idx != dim(buf)) {\n            next_index -= 1;\n\n            // pop and overwrite\n            poke(buf, peek(buf, next_index, 0), idx, 0);\n            poke(buf, peek(buf, next_index, 1), idx, 1);\n\n            // erase the last element with 0\n            poke(buf, 0, next_index, 0);\n            poke(buf, 0, next_index, 1);\n        }\n    }\n\n    return 0;\n}\n\n//============================================================\n// buffer\n\nBuffer buf(\"\");\n\n//============================================================\n// main process\n\nmidiPushPop(buf, in1, in2);",
